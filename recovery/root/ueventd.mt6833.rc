/dev/fw_log_bt            0660   bluetooth  bluetooth

# GPS
/dev/gpsdl0               0660   gps        gps
/dev/gpsdl1               0660   gps        gps
/dev/stpgps               0660   gps        gps
/dev/gps                  0660   gps        system
/dev/gps_emi              0660   gps        gps

# ANT
